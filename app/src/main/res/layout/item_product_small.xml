<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="wrap_content"    android:layout_height="wrap_content"    xmlns:app="http://schemas.android.com/apk/res-auto">        <com.google.android.material.card.MaterialCardView        android:id="@+id/cv_itemProductSmall_item"        android:layout_width="70dp"        android:layout_height="70dp"        android:backgroundTint="?backgroundLayout"        app:cardCornerRadius="12dp"        app:cardElevation="0dp"        android:layout_marginHorizontal="5dp"        android:layout_marginBottom="10dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        app:layout_constraintBottom_toBottomOf="parent">        <ImageView            android:id="@+id/iv_itemProductSmall_icon"            android:layout_width="match_parent"            android:layout_height="match_parent"            android:src="@drawable/ic_shop1"            android:padding="5dp"/>    </com.google.android.material.card.MaterialCardView>    <com.notion.presentation.cviews.TextViewRegular        android:id="@+id/tv_itemProductSmall_inventory"        android:layout_width="25dp"        android:layout_height="25dp"        android:background="@drawable/ovel_white"        android:text="3"        android:padding="5dp"        android:textSize="10sp"        android:elevation="1dp"        android:gravity="center"        android:layout_marginBottom="5dp"        app:layout_constraintTop_toBottomOf="@id/cv_itemProductSmall_item"        app:layout_constraintBottom_toBottomOf="@id/cv_itemProductSmall_item"        app:layout_constraintStart_toEndOf="@id/cv_itemProductSmall_item"        app:layout_constraintEnd_toEndOf="@id/cv_itemProductSmall_item"/></androidx.constraintlayout.widget.ConstraintLayout>