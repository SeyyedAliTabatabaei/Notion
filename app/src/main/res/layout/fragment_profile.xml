<?xml version="1.0" encoding="utf-8"?><androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="match_parent"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:background="?backgroundLayout"    android:layoutDirection="ltr">    <com.google.android.material.appbar.AppBarLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="?backgroundLayout"        app:liftOnScroll="true">        <androidx.constraintlayout.widget.ConstraintLayout            android:layout_width="match_parent"            android:layout_height="wrap_content">            <com.google.android.material.card.MaterialCardView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                app:cardCornerRadius="70dp"                app:layout_constraintTop_toTopOf="parent"                app:layout_constraintStart_toStartOf="parent"                android:layout_marginHorizontal="20dp"                app:cardElevation="0dp"                android:backgroundTint="?backgroundLayout"                app:strokeWidth="1dp"                app:strokeColor="?cardStrokColor"                android:layout_marginTop="24dp">                <LinearLayout                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:orientation="horizontal">                    <ImageView                        android:layout_width="44dp"                        android:layout_height="44dp"                        android:src="@drawable/ic_premium"                        android:layout_margin="4dp"/>                    <ImageView                        android:layout_width="44dp"                        android:layout_height="44dp"                        android:src="@drawable/ic_gifts"                        android:layout_margin="4dp"/>                </LinearLayout>            </com.google.android.material.card.MaterialCardView>            <ImageView                android:layout_width="50dp"                android:layout_height="50dp"                android:src="@drawable/ic_settings"                app:layout_constraintEnd_toEndOf="parent"                app:layout_constraintTop_toTopOf="parent"                android:layout_marginTop="24dp"                android:layout_marginHorizontal="20dp"/>        </androidx.constraintlayout.widget.ConstraintLayout>    </com.google.android.material.appbar.AppBarLayout>    <androidx.core.widget.NestedScrollView        android:layout_width="match_parent"        android:layout_height="match_parent"        app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior">        <LinearLayout            android:layout_width="match_parent"            android:layout_height="match_parent"            android:orientation="vertical">            <ImageView                android:layout_width="180dp"                android:layout_height="wrap_content"                android:adjustViewBounds="true"                android:src="@drawable/ic_avatar_premium"                android:layout_gravity="center_horizontal"                android:layout_marginTop="10dp"/>            <com.notion.presentation.cviews.TextViewBold                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="کاربر ويژه"                android:layout_gravity="center_horizontal"                android:textColor="?colorPrimary"/>            <com.notion.presentation.cviews.TextViewBold                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="سیدعلی طباطبایی"                android:textSize="20sp"                android:layout_gravity="center_horizontal"                android:layout_marginTop="5dp"/>            <com.notion.presentation.cviews.TextViewRegular                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="09151234563"                android:textSize="16sp"                android:textColor="?android:textColorSecondary"                android:layout_gravity="center_horizontal"                android:layout_marginTop="5dp"/>            <com.google.android.material.card.MaterialCardView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:backgroundTint="?backgroundCard"                android:layout_marginTop="16dp"                android:layout_marginHorizontal="20dp"                app:cardCornerRadius="12dp"                app:cardElevation="0dp">                <androidx.constraintlayout.widget.ConstraintLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layoutDirection="rtl">                    <ImageView                        android:id="@+id/iv_profile_medal"                        android:layout_width="60dp"                        android:layout_height="60dp"                        android:src="@drawable/ic_medal_one"                        app:layout_constraintStart_toStartOf="parent"                        app:layout_constraintTop_toTopOf="parent"                        app:layout_constraintBottom_toBottomOf="parent"                        android:layout_marginVertical="8sp"                        android:layout_marginHorizontal="12dp"/>                    <com.notion.presentation.cviews.TextViewRegular                        android:id="@+id/tv_profile_level"                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="سطح ۱۵"                        android:textSize="16sp"                        app:layout_constraintStart_toEndOf="@id/iv_profile_medal"                        android:layout_marginTop="6dp"                        app:layout_constraintTop_toTopOf="parent"                        android:layout_marginHorizontal="12dp"/>                    <ProgressBar                        android:id="@+id/pb_profile_level"                        android:layout_width="0dp"                        android:layout_height="10dp"                        style="@style/Widget.AppCompat.ProgressBar.Horizontal"                        android:progressDrawable="@drawable/round_progressbar"                        android:max="100"                        android:progress="41"                        android:layout_marginTop="5dp"                        android:layout_marginHorizontal="12dp"                        app:layout_constraintTop_toBottomOf="@id/tv_profile_level"                        app:layout_constraintEnd_toEndOf="parent"                        app:layout_constraintStart_toEndOf="@id/iv_profile_medal"/>                    <com.notion.presentation.cviews.TextViewRegular                        android:layout_width="wrap_content"                        android:layout_height="wrap_content"                        android:text="بدست آوردن درآمد ۱۵ هزار سکه"                        app:layout_constraintTop_toBottomOf="@id/pb_profile_level"                        app:layout_constraintStart_toEndOf="@id/iv_profile_medal"                        android:layout_marginTop="5dp"                        android:layout_marginHorizontal="12dp"                        android:textSize="12sp"                        app:layout_constraintBottom_toBottomOf="parent"                        android:layout_marginBottom="6dp"/>                </androidx.constraintlayout.widget.ConstraintLayout>            </com.google.android.material.card.MaterialCardView>            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="horizontal"                android:weightSum="3"                android:layout_marginTop="12dp"                android:layout_marginHorizontal="14dp">                <com.google.android.material.card.MaterialCardView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_weight="1"                    android:layout_marginHorizontal="6dp"                    app:cardCornerRadius="12dp"                    app:cardElevation="0dp">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical"                        android:gravity="center">                        <ImageView                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:src="@drawable/ic_wallet"                            android:adjustViewBounds="true"                            android:layout_marginTop="6dp"                            android:layout_marginHorizontal="20dp"/>                        <com.notion.presentation.cviews.TextViewBold                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="13,000"                            android:textSize="20sp"                            android:layout_marginTop="10dp"/>                        <com.notion.presentation.cviews.TextViewRegular                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="@string/inventory"                            android:textColor="?android:textColorSecondary"                            android:textSize="8sp"                            android:layout_marginBottom="12dp"/>                    </LinearLayout>                </com.google.android.material.card.MaterialCardView>                <com.google.android.material.card.MaterialCardView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_weight="1"                    android:layout_marginHorizontal="6dp"                    app:cardCornerRadius="12dp"                    app:cardElevation="0dp">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical"                        android:gravity="center">                        <ImageView                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:src="@drawable/ic_gem"                            android:adjustViewBounds="true"                            android:layout_marginTop="6dp"                            android:layout_marginHorizontal="20dp"/>                        <com.notion.presentation.cviews.TextViewBold                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="6"                            android:textSize="20sp"                            android:layout_marginTop="10dp"/>                        <com.notion.presentation.cviews.TextViewRegular                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="@string/your_gem"                            android:textColor="?android:textColorSecondary"                            android:textSize="8sp"                            android:layout_marginBottom="12dp"/>                    </LinearLayout>                </com.google.android.material.card.MaterialCardView>                <com.google.android.material.card.MaterialCardView                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_weight="1"                    android:layout_marginHorizontal="6dp"                    app:cardCornerRadius="12dp"                    app:cardElevation="0dp">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical"                        android:gravity="center">                        <ImageView                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:src="@drawable/ic_coin"                            android:adjustViewBounds="true"                            android:layout_marginTop="6dp"                            android:layout_marginHorizontal="20dp"/>                        <com.notion.presentation.cviews.TextViewBold                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="5,000"                            android:textSize="20sp"                            android:layout_marginTop="10dp"/>                        <com.notion.presentation.cviews.TextViewRegular                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="@string/your_coin"                            android:textColor="?android:textColorSecondary"                            android:textSize="8sp"                            android:layout_marginBottom="12dp"/>                    </LinearLayout>                </com.google.android.material.card.MaterialCardView>            </LinearLayout>            <com.notion.presentation.cviews.TextViewBold                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/games"                android:layout_marginHorizontal="20dp"                android:layout_marginTop="16sp"                android:layout_gravity="end"/>                        <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical"                android:layout_marginBottom="50dp"                android:layout_marginTop="12dp">                                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal"                    android:layout_marginHorizontal="15dp">                    <com.google.android.material.card.MaterialCardView                        android:layout_weight="1"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:backgroundTint="?backgroundCard"                        app:cardElevation="0dp"                        app:cardCornerRadius="12dp"                        android:layout_marginHorizontal="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:gravity="center">                            <ImageView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:src="@drawable/ic_dog"                                android:layout_marginTop="5dp"                                android:adjustViewBounds="true"                                android:padding="20dp"/>                            <com.notion.presentation.cviews.TextViewBlack                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="حیوانات"                                android:textSize="16sp"/>                        </LinearLayout>                    </com.google.android.material.card.MaterialCardView>                    <com.google.android.material.card.MaterialCardView                        android:layout_weight="1"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:backgroundTint="?backgroundCard"                        app:cardElevation="0dp"                        app:cardCornerRadius="12dp"                        android:layout_marginHorizontal="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:gravity="center">                            <ImageView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:src="@drawable/ic_sea"                                android:layout_marginTop="5dp"                                android:adjustViewBounds="true"                                android:padding="20dp"/>                            <com.notion.presentation.cviews.TextViewBlack                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="دریا"                                android:textSize="16sp"/>                        </LinearLayout>                    </com.google.android.material.card.MaterialCardView>                    <com.google.android.material.card.MaterialCardView                        android:layout_weight="1"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:backgroundTint="?backgroundCard"                        app:cardElevation="0dp"                        app:cardCornerRadius="12dp"                        android:layout_marginHorizontal="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:gravity="center">                            <ImageView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:src="@drawable/ic_hayola"                                android:layout_marginTop="5dp"                                android:adjustViewBounds="true"                                android:padding="20dp"/>                            <com.notion.presentation.cviews.TextViewBlack                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="هیولا"                                android:textSize="16sp"/>                        </LinearLayout>                    </com.google.android.material.card.MaterialCardView>                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="horizontal"                    android:layout_marginHorizontal="15dp"                    android:layout_marginTop="12dp">                    <com.google.android.material.card.MaterialCardView                        android:layout_weight="1"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:backgroundTint="?backgroundCard"                        app:cardElevation="0dp"                        app:cardCornerRadius="12dp"                        android:layout_marginHorizontal="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:gravity="center">                            <ImageView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:src="@drawable/ic_bomb"                                android:layout_marginTop="5dp"                                android:adjustViewBounds="true"                                android:padding="20dp"/>                            <com.notion.presentation.cviews.TextViewBlack                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="بمب"                                android:textSize="16sp"/>                        </LinearLayout>                    </com.google.android.material.card.MaterialCardView>                    <com.google.android.material.card.MaterialCardView                        android:layout_weight="1"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:backgroundTint="?backgroundCard"                        app:cardElevation="0dp"                        app:cardCornerRadius="12dp"                        android:layout_marginHorizontal="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:gravity="center">                            <ImageView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:src="@drawable/ic_attack"                                android:layout_marginTop="5dp"                                android:adjustViewBounds="true"                                android:padding="20dp"/>                            <com.notion.presentation.cviews.TextViewBlack                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="مبارزه"                                android:textSize="16sp"/>                        </LinearLayout>                    </com.google.android.material.card.MaterialCardView>                    <com.google.android.material.card.MaterialCardView                        android:layout_weight="1"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:backgroundTint="?backgroundCard"                        app:cardElevation="0dp"                        app:cardCornerRadius="12dp"                        android:layout_marginHorizontal="5dp">                        <LinearLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:orientation="vertical"                            android:gravity="center">                            <ImageView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:src="@drawable/ic_award"                                android:layout_marginTop="5dp"                                android:adjustViewBounds="true"                                android:padding="20dp"/>                            <com.notion.presentation.cviews.TextViewBlack                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:text="تاج"                                android:textSize="16sp"/>                        </LinearLayout>                    </com.google.android.material.card.MaterialCardView>                </LinearLayout>            </LinearLayout>        </LinearLayout>    </androidx.core.widget.NestedScrollView></androidx.coordinatorlayout.widget.CoordinatorLayout>